<% if (prescription.WellBeing && (prescription.WellBeing.questionAndAnswers.length || prescription.WellBeing.result)) { %>
<div class="flex flex-col my-4">
  <div class="text-xl font-medium mb-1 dark-light">
    Wellbeing Scale Score
  </div>
  <div class="flex flex-col gap-2 mt-2">

    <% prescription.WellBeing.questionAndAnswers.forEach(function(questionAnswer) { %>
    <div class="flex gap-x-2.5">
      <div>
        <img src="<%= imageThree %>" class="mt-2" />
      </div>
      <div class="flex flex-col">
        <div class="font-medium text-base font-quicksand text-black">
          <%= questionAnswer.question %>
        </div>
        <div class="text-base text-gray-700 font-roboto font-normal">
          <%= questionAnswer.answer %>
        </div>
      </div>
    </div>
    <% }) %>

    <% if (prescription.WellBeing.result) { %>
    <div class="flex flex-col">
      <div class="text-primary underline font-roboto font-semibold text-base">
        Result
      </div>

      <div class="text-gray-500 font-quicksand font-medium">
        ওয়েলবিং
        <%= prescription.WellBeing.result < 50 ? "খারাপ" : "ভাল" %>
        (রেজাল্ট ৫০% এর
        <%= prescription.WellBeing.result < 50 ? "কম" : "অধিক" %>)
      </div>
    </div>
    <% } %>
  </div>
</div>
<% } %>